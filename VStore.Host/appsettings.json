{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "LiterateConsole",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3}] [{ThreadId}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AWS": {
    "ServiceURL": "http://rgw.n3.hw",
    "LogResponse": true,
    "LogMetrics": true
  },
  "Ceph": {
    "TemplatesBucketName": "vstore-templates-01",
    "ObjectsBucketName": "vstore-objects-01",
    "FilesBucketName": "vstore-files-01",
    "Locks": {
      "BucketName": "vstore-locks-01",
      "Expiration": "00:30:00"
    }
  },
  "VStore": {
    "FileStorageEndpoint": "http://vstore-files-01.rgw.n3.hw"
  }
}

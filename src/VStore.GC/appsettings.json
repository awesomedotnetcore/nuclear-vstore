{
  "Serilog": {
    "MinimumLevel": null,
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AWS": {
    "ServiceURL": null,
    "LogMetrics": true,
    "LogResponse": false
  },
  "Ceph": {
    "TemplatesBucketName": null,
    "ObjectsBucketName": null,
    "FilesBucketName": null,
    "Locks": {
      "BucketName": null
    }
  },
  "VStore": {
    "FileStorageEndpoint": null
  },
  "Kafka": {
    "BrokerEndpoints": "127.0.0.1",
    "ObjectEventsTopic": "ams_vstore_stage.object.events",
    "ObjectVersionsTopic": "ams_vstore_stage.object.versions",
    "SessionsTopic": "ams_vstore_stage.session.events",
    "BinariesUsingsTopic": "ams_vstore_stage.object.binaries"
  }
}
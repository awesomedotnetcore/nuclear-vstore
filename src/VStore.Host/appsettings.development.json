{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "LiterateConsole",
        "Args": { "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3}] [{ThreadId}] {Message}{NewLine}{Exception}" }
      }
    ]
  },
  "AWS": {
    "ServiceURL": "http://rgw.n3.hw"
  },
  "Ceph": {
    "TemplatesBucketName": "vstore-templates-01",
    "ObjectsBucketName": "vstore-objects-01",
    "FilesBucketName": "vstore-files-01",
    "Locks": {
      "BucketName": "vstore-locks-01"
    }
  },
  "Kafka": {
    "BrokerEndpoints": "127.0.0.1",
    "ObjectEventsTopic": "ams_vstore_stage.object.events",
    "ObjectVersionsTopic": "ams_vstore_stage.object.versions",
    "SessionsTopic": "ams_vstore_stage.session.events",
    "BinariesReferencesTopic": "ams_vstore_stage.object.binaries"
  },
  "VStore": {
    "SessionExpiration": "00:10:00",
    "FileStorageEndpoint": "http://rgw.n3.hw/vstore-files-01/"
  }
}

{
  "Serilog": {
    "MinimumLevel": {
      "Default": null
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AWS": {
    "Region": "eu-central-1",
    "LogMetrics": false,
    "LogResponse": false
  },
  "Ceph": {
    "ServiceURL": null,
    "AccessKey": null,
    "SecretKey": null,
    "LogMetrics": false,
    "LogResponse": false,
    "DegreeOfParallelism": 8,
    "TemplatesBucketName": null,
    "ObjectsBucketName": null,
    "FilesBucketName": null
  },
  "DistributedLocks": {
    "EndPoints": [],
    "Expiration": "00:00:10"
  },
  "Kafka": {
    "BrokerEndpoints": null,
    "ObjectEventsTopic": null,
    "SessionEventsTopic": null
  },
  "VStore": {
    "SessionExpiration": "1:00:00:00",
    "FileStorageEndpoint": null,
    "MaxBinarySize": 10485760
  },
  "Jwt": {
    "Issuer": "okapi",
    "SecretKey": null
  }
}

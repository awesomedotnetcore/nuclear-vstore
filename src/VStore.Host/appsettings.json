{
  "Serilog": {
    "MinimumLevel": null,
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AWS": {
    "ServiceURL": null,
    "LogMetrics": true,
    "LogResponse": false
  },
  "Ceph": {
    "DegreeOfParallelism": 8,
    "TemplatesBucketName": null,
    "ObjectsBucketName": null,
    "FilesBucketName": null,
    "Locks": {
      "BucketName": null,
      "Expiration": "00:00:05"
    }
  },
  "Kafka": {
    "BrokerEndpoints": "127.0.0.1",
    "ObjectEventsTopic": "vstore-objects",
    "SessionsTopic": "vstore-sessions",
    "BinariesUsingsTopic": "vstore-binaries-usings"
  },
  "VStore": {
    "FileStorageEndpoint": null
  },
  "Jwt": {
    "Issuer": "okapi",
    "SecretKey": null
  }
}

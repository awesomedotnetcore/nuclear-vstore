{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3}] [{ThreadId}] {Message}{NewLine}{Exception}" }
      }
    ]
  },
  "AWS": {
    "LogMetrics": true
  },
  "Ceph": {
    "ServiceURL": "http://rgw.n3.hw",
    "LogMetrics": true,
    "TemplatesBucketName": "vstore-templates-stage",
    "ObjectsBucketName": "vstore-objects-stage",
    "FilesBucketName": "vstore-files-stage"
  },
  "Throttling": {
    "RequestTimeout": "00:05:00"
  }
}

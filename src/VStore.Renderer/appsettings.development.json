{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss:fff} {Level:u3}] {Message:j} <- {SourceContext} on thread {ThreadId}{NewLine}{Exception}" 
        }
      }
    ]
  },
  "AWS": {
    "LogMetrics": true
  },
  "Ceph": {
    "ServiceURL": "http://rgw.n3.hw",
    "LogMetrics": true,
    "FilesBucketName": "vstore-files-01"
  },
  "Throttling": {
    "RequestTimeout": "00:05:00"
  }
}

{
  "Serilog": {
    "MinimumLevel": {
      "Default": null
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },
  "AWS": {
    "Region": "eu-central-1",
    "LogMetrics": false,
    "LogResponse": false
  },
  "Ceph": {
    "ServiceURL": null,
    "AccessKey": null,
    "SecretKey": null,
    "LogMetrics": false,
    "LogResponse": false,
    "FilesBucketName": null
  },
  "ConnectionStrings": {
    "VersionedStorage": null
  },
  "Throttling": {
    "RequestTimeout": "00:00:10",
    "RetryAfter": "00:00:01",
    "MemoryLimit": 524288000,
    "ThresholdFactor": 0.8
  }
}
